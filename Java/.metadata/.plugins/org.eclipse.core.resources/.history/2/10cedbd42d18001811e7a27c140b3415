package aula04;

public class Aula {
	int x = 1;
	String y = "Olá";
	String z = "Mundo!";
	
	public static void main(String[] args) {
		Aula a = new Aula();
		int x2 = a.x + 2;
		
		System.out.println("Valor de x : " + a.x);
		System.out.println("Valor de x2: " + x2);
		System.out.println(a.y + " " + a.z	);
	}
	

}
